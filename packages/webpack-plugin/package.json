{
    "name": "@datadog/webpack-plugin",
    "packageManager": "yarn@4.0.2",
    "version": "0.0.1",
    "license": "MIT",
    "author": "Datadog",
    "main": "./dist/src/index.js",
    "type": "module",
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*.ts"
    },
    "publishConfig": {
        "access": "public",
        "exports": {
            "./package.json": "./package.json",
            ".": "./dist/src/index.js",
            "./*": "./dist/src/*.js"
        }
    },
    "files": [
        "dist/*"
    ],
    "scripts": {
        "build": "yarn clean && rollup --config rollup.config.js",
        "clean": "rm -rf dist",
        "prepack": "yarn build",
        "typecheck": "tsc --noEmit",
        "watch": "tsc -w"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "25.0.7",
        "@rollup/plugin-json": "6.1.0",
        "@rollup/plugin-node-resolve": "15.2.3",
        "esbuild": "0.20.2",
        "rollup": "4.17.1",
        "rollup-plugin-dts": "6.1.0",
        "rollup-plugin-esbuild": "6.1.1",
        "typescript": "5.4.3"
    },
    "dependencies": {
        "@datadog/build-plugins-core": "workspace:*",
        "@dd/factory": "workspace:*"
    },
    "peerDependencies": {
        "webpack": ">= 4.x < 6.x"
    }
}
