{
    "name": "@datadog/build-plugins-core",
    "packageManager": "yarn@4.0.2",
    "license": "MIT",
    "private": true,
    "author": "Datadog",
    "main": "./src/index.js",
    "exports": {
        "./hooks/datadog": "./src/hooks/datadog/index.ts",
        "./*": "./src/*.ts"
    },
    "publishConfig": {
        "access": "public",
        "exports": {
            "./package.json": "./package.json",
            "./hooks/datadog": "./dist/packages/core/src/hooks/datadog/index.js",
            "./*": "./dist/packages/core/src/*.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "yarn clean && tsc",
        "clean": "rm -rf dist",
        "prepack": "yarn build",
        "typecheck": "tsc --noEmit",
        "watch": "tsc -w"
    },
    "dependencies": {
        "chalk": "2.3.1",
        "esbuild": "0.20.2",
        "fs-extra": "7.0.1",
        "pretty-bytes": "5.6.0"
    },
    "devDependencies": {
        "@types/chalk": "2.2.0",
        "@types/fs-extra": "8.1.0",
        "@types/node": "13.13.4",
        "typescript": "5.4.3"
    }
}
