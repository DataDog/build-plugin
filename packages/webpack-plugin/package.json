{
    "name": "@datadog/webpack-plugin",
    "packageManager": "yarn@4.0.2",
    "private": true,
    "license": "MIT",
    "author": "Datadog",
    "main": "./dist/packages/webpack-plugin/src/index.js",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "import": "./src/index.ts",
            "require": "./src/index.ts"
        },
        "./*": {
            "import": "./src/*.ts",
            "require": "./src/*.ts"
        }
    },
    "files": [
        "dist/*"
    ],
    "scripts": {
        "build": "yarn clean; tsc",
        "clean": "rm -rf dist",
        "prepack": "yarn build",
        "typecheck": "tsc --noEmit",
        "watch": "tsc -w"
    },
    "devDependencies": {
        "typescript": "5.4.3"
    },
    "dependencies": {
        "@datadog/build-plugins-core": "workspace:*",
        "webpack": "5.49.0"
    },
    "peerDependencies": {
        "webpack": "*"
    },
    "peerDependenciesMeta": {
        "webpack": {
            "optional": true
        }
    }
}
